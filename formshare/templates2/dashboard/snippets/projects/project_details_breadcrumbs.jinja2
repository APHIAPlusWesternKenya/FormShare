{% extends 'dashboard/snippets/breadcrumbs.jinja2' %}

{% block breadcrumb_title %}
    <h2>{{ _("FormShare - Project list") }} - {{ projectName }}</h2>
{% endblock breadcrumb_title %}

{% block breadcrumbitems %}
    <li>
        <a href="{{ request.route_url('dashboard') }}">{{ _("Home") }}</a>
    </li>
    <li>
        <a href="{{ request.route_url('projects') }}">{{ _("Projects") }}</a>
    </li>
    <li class="active">
        <strong><a href="{{ request.route_url('project_details',projid=projectID) }}">{{ projectName }}</a></strong>
    </li>
{% endblock breadcrumbitems %}

