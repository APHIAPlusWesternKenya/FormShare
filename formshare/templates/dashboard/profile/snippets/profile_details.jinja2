<div class="row">
    {% block profile_user_section %}
        <div class="col-lg-3">
            {% block profile_user_section_about %}
                {% include 'dashboard/profile/snippets/about_tab.jinja2' %}
            {% endblock profile_user_section_about %}
            {% block profile_user_section_collaborators %}
                {% include 'dashboard/profile/snippets/collaboration_tab.jinja2' %}
            {% endblock profile_user_section_collaborators %}
            {% block profile_user_section_social %}
                {% include 'dashboard/profile/snippets/social_tab.jinja2' %}
            {% endblock profile_user_section_social %}
            {% block profile_user_section_extra %}

            {% endblock profile_user_section_extra %}
        </div>
    {% endblock profile_user_section %}
    {% block profile_project_section %}
        <div class="col-lg-5">
            {% include 'dashboard/projects/snippets/project_tab.jinja2' %}
            {% block profile_project_section_extra %}

            {% endblock profile_project_section_extra %}
        </div>
    {% endblock profile_project_section %}
    {% block profile_activity_section %}
        <div class="col-lg-4 m-b-lg">
            {% include 'dashboard/profile/snippets/timeline_tab.jinja2' %}
            {% block profile_activity_section_extra %}

            {% endblock profile_activity_section_extra %}
        </div>
    {% endblock profile_activity_section %}
</div>