<div class="feed-element">
    <a class="float-left">
        <div class="rounded-circle" style="margin-right: 10px; background-color: #0a6aa1; width: 38px; height: 38px; display: inline-block; vertical-align: middle" ><span style="color: white; position: relative; float: left; top: 50%; left: 50%; transform: translate(-50%, -50%);"><i class="fas fa-cog fa-2x"></i></span></div>
    </a>
    <div class="media-body ">
        Field name - Field desc<br>
        <small class="text-muted">Integer</small>
        <div class="actions">
            {% block field_item_processing_actions %}
                <a class="btn btn-xs btn-white cancel_repository"><i class="fas fa-ban" style="color: red;"></i> Block</a>
            {% endblock field_item_processing_actions %}
        </div>
    </div>
</div>