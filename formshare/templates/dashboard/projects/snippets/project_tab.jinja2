<div class="ibox">
    <div class="ibox-title">
        <h5>Projects</h5>
        <div class="ibox-tools">
            {% if activeUser != None %}
                <a href="{{ request.route_url('projects_add',userid=activeUser.login,_query={'next':request.url}) }}" class="btn btn-primary btn-xs" style="margin-right: 20px">{{ _('Create new project') }}</a>
            {% endif %}
            <a class="fullscreen-link">
                <i class="fas fa-expand" style="margin-right: 10px"></i>
            </a>
        </div>
    </div>
    <div class="ibox-content">
        {% include 'dashboard/projects/snippets/project_tab_content.jinja2' %}
    </div>
</div>

